    "marked": "4.2.4",
    "nanoid": "^4.0.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.34",
    "vis-data": "^7.1.6",
    "vis-network": "^9.1.6",
    "vue": "^3.2.45",
    "vue-codemirror": "^6.1.1",
